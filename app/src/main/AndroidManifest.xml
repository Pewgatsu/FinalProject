<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobileprogfinalproject">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon_test"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_icon_test"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashScreenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginPage"
            android:theme="@style/LoginPageTheme" />
        <activity android:name=".AboutPage" />
        <activity
            android:name=".RegisterPage"
            android:theme="@style/registerTheme"/>
        <activity android:name=".AddPasswordPage"></activity>
        <activity
            android:name=".HomePage"
            android:label="@string/title_activity_home_page"
            android:theme="@style/AppTheme.NoActionBar"></activity>


    </application>

</manifest>